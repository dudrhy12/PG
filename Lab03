#include <stdio.h>

int main()
{
	int prime[1000];
	int i, p, q;

	prime[0] = prime[1] = 0;

	for (p = 2; p <= 999; p = p + 1) {
		prime[p] = 1;
	}

	for (p = 2; p * p < 1000; p = p + 1) {
		if (prime[p] == 1) {
			for (q = 2; p * q < 1000; q = q + 1) {
				prime[p * q] = 0;
			}
		}
	}

	for (p = 0, i = 0; p <= 999; p = p + 1) {
		if (prime[p] == 1) {
			printf("%3d\t", p);
			i = i + 1;
			if (i % 15 == 0) {
				printf("\n");
			}
		}
	}
}
